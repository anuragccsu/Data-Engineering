	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "openfda-adf2"
		},
		"AzureBlobStorageForLocalSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageForLocalSQLServer'"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_adls_generic_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_generic'"
		},
		"ls_azure_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql_db'"
		},
		"ls_OpenFDA_REST_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.fda.gov/"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://openfda.dfs.core.windows.net/"
		},
		"ls_adls_generic_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://localsqlstorageforadf.dfs.core.windows.net/"
		},
		"ls_azure_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "avcovid-srv.database.windows.net"
		},
		"ls_azure_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure_sql_db"
		},
		"ls_azure_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adm"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
