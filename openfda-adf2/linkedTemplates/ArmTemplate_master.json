{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "openfda-adf2"
		},
		"AzureBlobStorageForLocalSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageForLocalSQLServer'"
		},
		"LocalSQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LocalSQLServer'"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_adls_generic_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_generic'"
		},
		"ls_azure_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql_db'"
		},
		"LocalSQLServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "OVIYA\\AVMSSQL_1"
		},
		"LocalSQLServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2019"
		},
		"LocalSQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adf_user"
		},
		"ls_OpenFDA_REST_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.fda.gov/"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://openfda.dfs.core.windows.net/"
		},
		"ls_adls_generic_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://localsqlstorageforadf.dfs.core.windows.net/"
		},
		"ls_azure_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "avcovid-srv.database.windows.net"
		},
		"ls_azure_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure_sql_db"
		},
		"ls_azure_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adm"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "openfda-adf2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageForLocalSQLServer_connectionString": {
						"value": "[parameters('AzureBlobStorageForLocalSQLServer_connectionString')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_adls_generic_accountKey": {
						"value": "[parameters('ls_adls_generic_accountKey')]"
					},
					"ls_azure_sql_db_password": {
						"value": "[parameters('ls_azure_sql_db_password')]"
					},
					"ls_OpenFDA_REST_properties_typeProperties_url": {
						"value": "[parameters('ls_OpenFDA_REST_properties_typeProperties_url')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_adls_generic_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_generic_properties_typeProperties_url')]"
					},
					"ls_azure_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_server')]"
					},
					"ls_azure_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_database')]"
					},
					"ls_azure_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "openfda-adf2_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LocalSQLServer_password": {
						"value": "[parameters('LocalSQLServer_password')]"
					},
					"LocalSQLServer_properties_typeProperties_server": {
						"value": "[parameters('LocalSQLServer_properties_typeProperties_server')]"
					},
					"LocalSQLServer_properties_typeProperties_database": {
						"value": "[parameters('LocalSQLServer_properties_typeProperties_database')]"
					},
					"LocalSQLServer_properties_typeProperties_userName": {
						"value": "[parameters('LocalSQLServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'openfda-adf2_ArmTemplate_0')]"
			]
		}
	]
}